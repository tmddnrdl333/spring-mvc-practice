<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <properties resource="database_for_generator.properties"/>
    <context id="SteveJobda" targetRuntime="MyBatis3">

        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="org.mybatis.generator.plugins.ToStringPlugin" />
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />

        <jdbcConnection driverClass="${jdbc.driverClassName}" connectionURL="${jdbc.url}" userId="${jdbc.username}"
                        password="${jdbc.password}"/>

        <javaModelGenerator
                targetPackage="com.applemango.SteveJobda.domain"
                targetProject="/MIDASIT/SteveJobda/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.mapper"
                         targetProject="/MIDASIT/SteveJobda/src/main/resources"/>

        <javaClientGenerator
                targetPackage="com.applemango.SteveJobda.persistence.mapper"
                targetProject="/MIDASIT/SteveJobda/src/main/java" type="XMLMAPPER"/>

        <table tableName="admin" domainObjectName="Admin"><property name="runtimeSchema" value=""/></table>
        <table tableName="img" domainObjectName="Img"><property name="runtimeSchema" value=""/></table>
        <table tableName="posting" domainObjectName="Posting"><property name="runtimeSchema" value=""/></table>
        <table tableName="auth" domainObjectName="Auth"><property name="runtimeSchema" value=""/></table>
        <table tableName="admin_auth" domainObjectName="AdminAuth"><property name="runtimeSchema" value=""/></table>

    </context>
</generatorConfiguration>